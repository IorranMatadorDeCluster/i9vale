{
  "name": "imoveis-api",
  "version": "1.0.0",
  "description": "API to fetch and parse real estate data from XML to JSON",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "docker:build": "docker build -t imoveis-api .",
    "docker:build:simple": "docker build -f Dockerfile.simple -t imoveis-api .",
    "docker:run": "docker run -p 3000:3000 imoveis-api",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "keywords": ["real-estate", "api", "xml", "json"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "xml2js": "^0.6.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/xml2js": "^0.4.14",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.9",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1"
  }
} 