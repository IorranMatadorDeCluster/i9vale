CREATE TABLE "public"."imoveis_vale" ( 
  "id" SERIAL,
  "codigo_imovel" VARCHAR(50) NOT NULL,
  "finalidade" VARCHAR(50) NULL,
  "tipo_imovel" VARCHAR(50) NOT NULL,
  "cidade" VARCHAR(100) NOT NULL,
  "tipo_oferta" VARCHAR(20) NOT NULL,
  "preco_venda" NUMERIC NULL,
  "preco_aluguel" NUMERIC NULL,
  "qtd_dormitorios" INTEGER NULL,
  "qtd_suites" INTEGER NULL,
  "area_util" NUMERIC NULL,
  "bairro" VARCHAR(100) NULL,
  "qtd_vagas_cobertas" INTEGER NULL,
  "qtd_banheiros" INTEGER NULL,
  "padrao_imovel" VARCHAR(50) NULL,
  "padrao_localizacao" VARCHAR(50) NULL,
  "ano_construcao" INTEGER NULL,
  "mobiliado" BOOLEAN NULL DEFAULT false ,
  "ar_condicionado" BOOLEAN NULL DEFAULT false ,
  "piscina" BOOLEAN NULL DEFAULT false ,
  "elevador" BOOLEAN NULL DEFAULT false ,
  "estado" VARCHAR(2) NULL,
  "endereco" TEXT NULL,
  "titulo_imovel" VARCHAR(255) NULL,
  "preco_condominio" NUMERIC NULL,
  "observacoes" TEXT NULL,
  "ativo" BOOLEAN NULL DEFAULT true ,
  "data_cadastro" TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ,
  "data_atualizacao" TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ,
  "url_site" VARCHAR NULL,
  CONSTRAINT "imoveis_vale_pkey" PRIMARY KEY ("id"),
  CONSTRAINT "imoveis_vale_codigo_imovel_key" UNIQUE ("codigo_imovel")
);